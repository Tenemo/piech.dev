{
    "name": "piech.dev",
    "version": "1.0.0",
    "author": "Piotr Piech <piotr@piech.dev>",
    "license": "UNLICENSED",
    "description": "Personal portfolio page.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tenemo/piech.dev.git"
    },
    "bugs": {
        "url": "https://github.com/Tenemo/piech.dev/issues"
    },
    "homepage": "https://github.com/Tenemo/piech.dev#readme",
    "scripts": {
        "predev": "node --experimental-strip-types src/utils/fetchGitHubData.ts",
        "dev": "react-router dev",
        "prebuild": "node --experimental-strip-types src/utils/fetchGitHubData.ts && npm run lint && npm run tsc && npm test",
        "build": "react-router build",
        "postbuild": "node --experimental-strip-types src/utils/transformImagePaths.ts && rimraf dist/server && rimraf dist/assets && rimraf --glob \"dist/client/**/*.js\"",
        "prebuild:skip": "node --experimental-strip-types src/utils/fetchGitHubData.ts",
        "build:skip": "react-router build",
        "postbuild:skip": "npm run postbuild",
        "build:analyze": "cross-env ANALYZE=true react-router build",
        "eslint": "eslint . -c eslint.config.js",
        "eslint:fix": "eslint . --fix -c eslint.config.js",
        "github-refetch": "node --experimental-strip-types src/utils/fetchGitHubData.ts --refetch",
        "stylelint": "stylelint **/*.scss **/*.css",
        "prelint": "node --experimental-strip-types src/utils/fetchGitHubData.ts",
        "lint": "npm run eslint && npm run stylelint",
        "preview": "vite preview --outDir dist/client",
        "tsc": "tsc",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "prepare": "husky"
    },
    "browserslist": [
        "> 1%",
        "not IE > 0",
        "not op_mini all"
    ],
    "engines": {
        "node": ">=22.18.0"
    },
    "type": "module",
    "dependencies": {
        "@dr.pogodin/react-helmet": "^3.0.2",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.1",
        "@sentry/browser": "^10.5.0",
        "@sentry/react": "^10.5.0",
        "@sentry/react-router": "^10.5.0",
        "date-fns": "^4.1.0",
        "history": "^5.3.0",
        "normalize.css": "^8.0.1",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-error-boundary": "^6.0.0",
        "react-markdown": "^10.1.0",
        "react-router": "^7.8.1",
        "react-syntax-highlighter": "^15.6.1",
        "rehype-raw": "^7.0.0",
        "remark-gfm": "^4.0.1"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.33.0",
        "@julr/vite-plugin-validate-env": "^2.2.0",
        "@react-router/dev": "^7.8.1",
        "@react-router/node": "^7.8.1",
        "@react-router/serve": "^7.8.1",
        "@testing-library/jest-dom": "^6.7.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/eslint": "^9.6.1",
        "@types/node": "^24.3.0",
        "@types/react": "^19.1.10",
        "@types/react-dom": "^19.1.7",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@vitejs/plugin-react": "^5.0.0",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/eslint-plugin": "^1.3.4",
        "autoprefixer": "^10.4.21",
        "browserslist-to-esbuild": "^2.1.1",
        "cross-env": "^10.0.0",
        "cssnano": "^7.1.0",
        "dotenv": "^17.2.1",
        "eslint": "9.33",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-only-error": "^1.0.2",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "globals": "^16.3.0",
        "html-webpack-plugin": "^5.6.4",
        "husky": "^9.1.7",
        "jsdom": "^26.1.0",
        "postcss-flexbugs-fixes": "^5.0.2",
        "prettier": "^3.6.2",
        "prettier-eslint": "^16.4.2",
        "prettier-stylelint": "^0.4.2",
        "rimraf": "^6.0.1",
        "rollup-plugin-visualizer": "^6.0.3",
        "sass-embedded": "^1.90.0",
        "stylelint": "^16.23.1",
        "stylelint-config-recommended": "^17.0.0",
        "stylelint-config-recommended-scss": "^16.0.0",
        "stylelint-scss": "^6.12.1",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.1",
        "vite": "^7.1.2",
        "vite-css-modules": "^1.10.0",
        "vite-plugin-css-injected-by-js": "^3.5.2",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.46.2"
    }
}
